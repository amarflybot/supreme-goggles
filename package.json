{
  "name": "supreme-goggles",
  "version": "0.0.1",
  "description": "A sample monorepo for nestjs",
  "scripts": {
    "prepare": "husky install",
    "cz": "git-cz",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\" \"test/**/*.ts\"",
    "build": "lerna run build",
    "lint": "lerna run lint",
    "test": "lerna run test",
    "test:cov": "lerna run test:cov",
    "test:e2e": "lerna run test:e2e"
  },
  "devDependencies": {
    "concurrently": "5.0.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "monorepo",
    "typescript"
  ],
  "author": "Amarendra Kumar"
}
